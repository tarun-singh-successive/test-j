                    @Html.LabelFor(m => m.TransactionDate, new { @class = "col-md-5 col-md-offset-1 control-label" })
                    <div class="col-md-6">
                        @Html.DisplayFor(m => m.TransactionDateString)
                    </div>
                </div>

                <div class="form-group view-form">
                    @Html.LabelFor(m => m.Id, new { @class = "col-md-5 col-md-offset-1 control-label" })
                    <div class="col-md-6">
                        @Html.DisplayFor(m => m.Id)
                    </div>
                </div>
