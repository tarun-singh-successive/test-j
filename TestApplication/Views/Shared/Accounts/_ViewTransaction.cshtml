                    @Html.LabelFor(m => m.TransactionType, new { @class = "col-md-5 col-md-offset-1 control-label" })
                    <div class="col-md-6">
                        @Html.DisplayFor(m => m.TransactionTypeString)
                    </div>
                </div>
                <div class="form-group view-form">
                    @Html.LabelFor(m => m.Amount, new { @class = "col-md-5 col-md-offset-1 control-label" })
                    <div class="col-md-6">
                        @Html.DisplayFor(m => m.Amount)
                    </div>
                </div>
                <div class="form-group view-form">
                    @Html.LabelFor(m => m.StatusId, new { @class = "col-md-5 col-md-offset-1 control-label" })
                    <div class="col-md-6">
                        @Html.DisplayFor(m => m.StatusText)
                    </div>
                </div>
                <div class="form-group view-form">
                    @Html.LabelFor(m => m.PaymentModeId, new { @class = "col-md-5 col-md-offset-1 control-label" })
