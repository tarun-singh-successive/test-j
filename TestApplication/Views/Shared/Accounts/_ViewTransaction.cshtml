<div class="col-md-12">
    <div class="card">
        <div class="card-body" style="padding:10px;">
            <div class="form-horizontal">
                <div class="form-group view-form">
                    @Html.LabelFor(m => m.MemberAccount.Member.Name, new { @class = "col-md-5 col-md-offset-1 control-label" })
                    <div class="col-md-6">
                        @Html.DisplayFor(m => m.MemberAccount.Member.Name)
                    </div>
                </div>

                <div class="form-group view-form">
                    @Html.Label("Account", new { @class = "col-md-5 col-md-offset-1 control-label" })
